    "eslint": "^5.15.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0"
  },
  "scripts": {
    "start": "npm run build && node dist/bin/www.js",
    "clean": "rm -rf dist",
